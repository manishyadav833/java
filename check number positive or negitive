package Statement;
import java.util.*;
public class pos_and_neg_number {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        if(n>=0){
            System.out.println("Positive Number");
        }
        else{
            System.out.println("Negitive number");
        }
    }
}
