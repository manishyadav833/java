package Loops;

import java.util.Scanner;

public class revese {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int rev=0;
        do{
            int digit=n%10;
            rev=rev * 10 +digit;
            // n ko 10 se divided karke n ki jagha rakh denge
            n /=10;
        }while (n !=0);
        System.out.println(rev);
    }
}
