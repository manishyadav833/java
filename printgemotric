package Loops;

import java.util.Scanner;

public class gemotric {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
        int a=1,r=2;
        for(int i=0;i<=n;i++){
           a *= r;
            System.out.println(a);
        }
    }
}
