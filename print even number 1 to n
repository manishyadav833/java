package Statement;
import java.util.*;
public class onlyeven_number_1_to_N {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.print("Value of N:");
        for (int i = 1; i <= n; i++) {
            if (i%2 == 0) {
                System.out.println("Even Number:" +i);
            }

        }
    }
}
